    <!-- Front End MetaData -->
    <em:name>ImgDB</em:name>
    <em:description>ImgDB: An Image Database not really in the clouds</em:description>
    <em:creator>Pierre Lindenbaum</em:creator>
    <em:homepageURL>http://plindenbaum.blogspot.com</em:homepageURL>
  </Description>
</RDF>
